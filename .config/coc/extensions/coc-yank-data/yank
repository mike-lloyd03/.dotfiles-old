	def commonCharacterCount(s1, s2):
	    l1 = list(s1)
	    l2 = list(s2)
	    com_list = []
	    for i in l1:
	        if i in l2:
	            com_list.append(l2.pop(l2.index(i)))
	    return len(com_list)
