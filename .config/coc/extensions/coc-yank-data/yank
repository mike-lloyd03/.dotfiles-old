1ee026cdcdc8d9cdf6744ec6d5892a94|file:///Users/mikelloyd/Documents/coding/playground/playground.py|3|1|V|python
	def increasingSequence(lst):
	    wlst = lst[:]
	    wlst_sorted = wlst[:]
	    wlst_sorted.sort()
	    return wlst == list(set(wlst_sorted))
	
	def drop(lst, i):
	    return lst[:i] + lst[i+1:]
	
	def almostIncreasingSequence(lst):
	    if len(lst) == 1:
	        return True
	    if len(lst) == 2:
	        return lst[1] > lst[0]
	    for i in range(len(lst) - 1):
	        if lst[i] > lst[i + 1]:
	            a = increasingSequence(drop(lst, i))
	            b = increasingSequence(drop(lst, i + 1))
	            print(lst[i], a, b)
	            if not any([a, b]):
	                return False
	    return True
