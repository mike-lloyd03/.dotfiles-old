	def patternChecker(pattern, string):
	    vowels = 'a e i o u y'.split(' ')
	    string_binary = ['0' if c in vowels else '1' for c in string]
	    string_binary = ''.join(string_binary)
	    print(string_binary)
	    return pattern == string_binary
	    
	def binaryPatternMatching(pattern, s):
	    result = 0
	    for i in range(len(pattern) - len(s)):
	        result = result + patternChecker(pattern, s[i:i + len(s)])
	    return result
