	            return inputString[0:i] + reverseInParentheses(inputString[i + 1:])
	        elif inputString[i] == ')':
	            return inputString[:i][::-1] + reverseInParentheses(inputString[i + 1:])
	        else:
	            i = i + 1
542a11d99027e3d7b36d8ba940f7be0c|file:///Users/mikelloyd/Documents/coding/playground/playground.py|4|1|V|python
	def reverseInParentheses(inputString):
	    regex = r'\([a-z]*\)'
	    m = re.search(regex, inputString)
	    if m:
	        print(m[0])
	        s, e = m.span()
	        flipped = inputString[:s] + m[0][-2:0:-1] + inputString[e:]
	        return reverseInParentheses(flipped)
	    else:
	        return inputString
eb34fc919019279a7dd79c880fc3465e|file:///Users/mikelloyd/Documents/coding/playground/playground.py|2|1|V|python
	import re
	
	def reverseInParentheses(inputString):
	    regex = r'\([a-z]*\)'
	    m = re.search(regex, inputString)
	    if m:
	        print(m[0])
	        s, e = m.span()
	        flipped = inputString[:s] + m[0][-2:0:-1] + inputString[e:]
	        return reverseInParentheses(flipped)
	    else:
	        return inputString
